    "jsdom": "^26.1.0",
    "temporal-polyfill": "^0.3.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.3",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.10"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {}
