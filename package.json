{
  "name": "cullendar",
  "description": "The completely headless, high-performance resource calendar built with Vue3.",
  "private": false,
  "version": "0.0.19",
  "repository": {
    "type": "git",
    "url": "https://github.com/niallbarrett/cullendar.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/cullendar.umd.cjs",
  "module": "./dist/cullendar.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
